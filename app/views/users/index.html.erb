<h1>Users</h1>

 <% @users.each do |user| %>

 <div class="user">
   <%= render "shared/avatar", user: user, width: "100px"%>

   <p><%= link_to  "@#{user.username}", user_path(id: user.id) %></p>
   <p><%= user.first_name %></p>
   <p><%= user.last_name %></p>
   <p><%= user.city %></p>
   <p><%= user.state %></p>

 </div>

 <% end %>

</div>
